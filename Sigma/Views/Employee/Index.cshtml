@model IList<Models.DomainModels.Employee>

@{
    ViewBag.Title = "Employee List View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-content">
    <h3 class="page-title">
        Employee List
    </h3>
    <div class="page-bar">
        <ul class="page-breadcrumb">
            <li>
                <i class="fa fa-home"></i>
                <a href="/Dashboard/Index">Home</a>
                <i class="fa fa-angle-right"></i>
            </li>
            <li>
                <a href="javascript:;">Employee</a>
            </li>
        </ul>
    </div>
    <div class="row">
        <div class="col-md-12">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Designation</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var employee in Model)
                    {
                        <tr>
                            <td>@employee.EmployeeName</td>
                            <td>@employee.EmployeeDesignation</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>


